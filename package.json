    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "typewriter-effect": "^2.10.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
